<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ca.hec.opensyllabus2.api.model.template.RubricI18nLabel" table="OSYL2_RUBRICI18NLABEL">
    
    <composite-id>
    	<key-property name="rubric_id" column="RUBRIC_ID" />
    	<key-property name="locale" column="LOCALE"/>
    </composite-id>
     
     <property name="label" type="string">
            <column name="LABEL" />
     </property>
    </class>
</hibernate-mapping>
