<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">



<hibernate-mapping>
    <class name="ca.hec.opensyllabus2.api.model.syllabus.Syllabus" table="OSYL2_SYLLABUS">
        <id name="syllabus_id" type="java.lang.Long">
            <column name="SYLLABUS_ID" />
            <generator class="native">
            	<param name="sequence">SYLLABUS_ID_SEQ</param>
            </generator>
        </id>
        <property name="course_id" type="java.lang.String">
            <column name="COURSE_ID" />
        </property>
        <property name="courseTitle" type="java.lang.String">
            <column name="COURSETITLE" />
        </property>
        <property name="template_id" type="java.lang.Long">
            <column name="TEMPLATE_ID" />
        </property>
        <property name="locale" type="java.lang.String">
            <column name="LOCALE" />
        </property>
        <property name="createdBy" type="java.lang.String">
            <column name="CREATEDBY" />
        </property>
        <property name="creationDate" type="java.util.Date">
         	<column name="CREATIONDATE" />
        </property>
    </class>
</hibernate-mapping>
