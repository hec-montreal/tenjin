<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ca.hec.opensyllabus2.api.model.template.TemplateElement" table="OSYL2_TEMPLATEELEMENT">
        <id name="id" type="long">
            <column name="TEMPLATEELEMENT_ID" />
            <generator class="native">
                 <param name="sequence">TEMPLATEELEMENT_ID_SEQ</param>
        	</generator>
        </id>
        <property name="defaultLabel" type="string" not-null="true">
        	<column name="DEFAULT_LABEL"/>
        </property>
        <many-to-one name="type" column="ELEMENTTYPE_ID" lazy="false"
            class="ca.hec.opensyllabus2.api.model.template.TemplateElementType"/>
    </class>
</hibernate-mapping>
