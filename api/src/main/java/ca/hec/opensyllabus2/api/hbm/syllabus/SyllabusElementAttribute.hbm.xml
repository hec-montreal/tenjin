<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ca.hec.opensyllabus2.api.model.syllabus.SyllabusElementAttribute" table="OSYL2_SYLLABUSELEMENTATTRIBUTE">
     
        <id name="elementAttribute_id" type="long" >
            <column name="ELEMENTATTRIBUTE_ID" />
            <generator class="native">
            	<param name="sequence">ELEMENTATTRIBUTE_ID_SEQ</param>
            </generator>
        </id>
        
        <property name="syllabusStructure_id" type="long" >
            <column name="SYLLABUSSTRUCTURE_ID" />           
        </property>
        <property name="attributeKey" type="string" >
            <column name="ATTRIBUTEKEY" />
        </property>
        <property name="attributeValue" type="text" length="100">
            <column name="ATTRIBUTEVALUE" />
        </property>
    </class>
</hibernate-mapping>
