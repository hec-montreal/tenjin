<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ca.hec.opensyllabus2.api.model.template.Rubric" table="OSYL2_RUBRIC">
        <id name="rubric_id" type="java.lang.Long">
            <column name="RUBRIC_ID" />
            <generator class="native">
            	<param name="sequence">osyl2_RUBRIC_ID</param>
            </generator>
        </id>
        <property name="label" type="java.lang.String">
            <column name="LABEL" />
        </property>
    </class>
</hibernate-mapping>
