<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ca.hec.opensyllabus2.api.model.syllabus.SyllabusElementSection" table="OSYL2_SYLLABUSELEMENTSECTION">
       <id name="elementSection_id" type="java.lang.Long">
            <column name="ELEMENTSECTION_ID" />
            <generator class="native">
            	<param name="sequence">ELEMENTSECTION_ID_SEQ</param>
            </generator>
        </id>
        <property name="section_id" type="java.lang.String">
            <column name="SECTION_ID" />
        </property>
         <property name="syllabusStructure_id" type="java.lang.Long">
            <column name="SYLLABUSSTRUCTURE_ID" />
        </property>
    </class>
</hibernate-mapping>
